{
  "AppSettings": {
    "Secret": "",
    "TokenExpirationInDays": 7,
    "RevokedTokens": [],
    "AuthProvider": "Sqlite",
    "RegistryProvider": "Sqlite",
    "StorageProvider": {
      "type": "Physical",
      "settings": {
        "path": "./App_Data/storage"
      }
    },
    "DefaultAdmin": {
      "Email": "admin@example.com",
      "UserName": "admin",
      "Password": "password"
    },
    "DdbStoragePath": "./App_Data/ddb",
    "MaxRequestBodySize": 52428800,
    "MaxUploadChunkSize": 512000,
    "SupportedDdbVersion": {
      "Major": 0,
      "Minor": 9,
      "Build": 4
    },
    "BatchTokenLength": 32,
    "RandomDatasetNameLength": 16
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityConnection": "Data Source=App_Data/identity.db;Mode=ReadWriteCreate",
    "RegistryConnection": "Data Source=App_Data/registry.db;Mode=ReadWriteCreate"
  }
}